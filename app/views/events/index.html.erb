<h1>All Your Events, Homie</h1>

<h2>current user is below</h2>
<h1><%= current_user.id %></h1>

<ul>
  <% @events.each do |event| %>
    <li><%= event %></li>
    <li><%= event.creator_id %></li> 
    <li><%= event.date %></li>
    <li><%= event.location %></li>
    <li><%= link_to "show event page", event_url(event) %></li>
  <% end %>
</ul>
